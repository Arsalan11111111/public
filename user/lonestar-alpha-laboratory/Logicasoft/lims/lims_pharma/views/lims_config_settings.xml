<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">General Settings</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="lims_base.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <div data-key="lims_base" position="inside">
                <h2>lims pharma</h2>
                <div class="row mt16 o_settings_container" name="lims_pharma_config">
                    <group>
                        <field name="unquantifiable_text"/>
                    </group>
                </div>
            </div>
        </field>
    </record>
</odoo>