<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record model="ir.ui.view" id="parameter_print_form">
        <field name="name">Parameter Print</field>
        <field name="model">lims.parameter.print</field>
        <field name="inherit_id" ref="lims_report.parameter_print_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='is_default_print_on_report']" position="after">
                <field name="print_mean_report"/>
            </xpath>
        </field>
    </record>

</odoo>
