<odoo>

    <record id="res_config_settings_form_lims_web" model="ir.ui.view">
        <field name="name">General Settings</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="lims_base.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <div data-key="lims_base" position="inside">
                <h2>lims web</h2>
                <div class="row mt16 o_settings_container" name="lims_config_boolean">
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane">
                            <field name="has_reworkable_results_web"/>
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="has_reworkable_results_web"/>
                            <div class="text-muted">
                                Reworkable results on web
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </field>
    </record>

</odoo>