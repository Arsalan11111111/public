<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="lims_parameter_pack_line_item_tree" model="ir.ui.view">
        <field name="name">Pack line item</field>
        <field name="model">lims.parameter.pack.line.item</field>
        <field name="arch" type="xml">
            <tree>
                <field name="sequence" widget="handle"/>
                <field name="parent_pack_of_pack_id"/>
                <field name="pack_id"/>
                <field name="rel_matrix_id"/>
                <field name="rel_regulation_id"/>
                <field name="rel_labo_id"/>
                <field name="rel_department_id"/>
                <field name="rel_active"/>
                <field name="rel_state"/>
            </tree>
        </field>
    </record>

</odoo>
