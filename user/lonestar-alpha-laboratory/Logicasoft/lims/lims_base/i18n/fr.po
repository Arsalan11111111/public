msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                    Hello,\n"
"                    <br>\n"
"                    We inform you that the analyses of your request :<b> <t t-out=\"object.name or ''\"></t></b>,\n"
"                    <br>\n"
"                    <p t-if=\"object.request_date\">\n"
"                        requested on <b t-out=\"object.request_date\"></b>\n"
"                    </p>\n"
"                </p>\n"
"            </div>\n"
"            <p>\n"
"            <t t-if=\"object.analysis_count\">\n"
"                which include:\n"
"                <br>\n"
"                <ul>\n"
"                    <t t-as=\"line\" t-foreach=\"object.sample_ids\">\n"
"                        <li t-if=\"line.analysis_id\">\n"
"                            <t t-out=\"line.analysis_id.sample_name or ''\"/> <t t-out=\"line.analysis_id.matrix_id.name or ''\"/> <t t-out=\"line.analysis_id.name or ''\"/>\n"
"                        </li>\n"
"                        <li t-else=\"\">\n"
"                            <b><t t-out=\"line.name or ''\"/></b> <t t-out=\"line.matrix_id.name or ''\"/>\n"
"                        </li>\n"
"                    </t>\n"
"                </ul>\n"
"            </t>\n"
"            have been received.\n"
"            <br>\n"
"            Do not hesitate to contact us if you have any questions.\n"
"            <br>\n"
"            </p>\n"
"        "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                    Bonjour,\n"
"                    <br/>\n"
"                    Nous vous informons que les analyses de votre demande :<b> <t t-out=\"object.name or ''\"/></b>,\n"
"                    <br/>\n"
"                    <p t-if=\"object.request_date\">\n"
"                        demandées le <b t-out=\"object.request_date\"/>\n"
"                    </p>\n"
"                </p>\n"
"            </div>\n"
