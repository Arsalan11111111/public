        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="lims_base.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <div data-key="lims_base" position="inside">
                <h2>lims SPW Tax</h2>
                <div class="row mt16 o_settings_container" name="lims_spw_tax_config">
                    <group>
                        <field name="years_create_tax"/>
                    </group>
                </div>
            </div>
        </field>
    </record>
</odoo>