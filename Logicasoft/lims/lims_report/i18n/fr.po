msgid ""
"<div style=\"margin:0px 0 1rem 0; font-size:13px; font-family:'Lucida Grande', Helvetica, Verdana, Arial, sans-serif;\">\n"
"                <h3>Analysis draft report <em>(<t t-out=\"object.name or ''\"></t>)</em></h3>\n"
"                <br>\n"
"                <p>Dear <t t-out=\"object.partner_id.title.name or ''\"></t> <t t-out=\"object.partner_id.name or ''\"></t></p>\n"
"                <p>Please find enclosed your report <t t-out=\"object.name or ''\"></t></p>\n"
"                <t t-if=\"object.report_analysis_line_ids\">\n"
"                    <br>\n"
"                    <p><em>This draft report includes the results of the samples :</em></p>\n"
"                    <ul>\n"
"                        <t t-foreach=\"object.report_analysis_line_ids\" t-as=\"line\">\n"
"                            <li><t t-out=\"line.analysis_id.name or ''\"></t> <t t-out=\"line.analysis_id.description or ''\"></t></li>\n"
"                        </t>\n"
"                    </ul>\n"
