<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record model='ir.ui.view' id='lims_analysis_report_line_tree_view'>
        <field name="name">Analysis Report Line</field>
        <field name="model">lims.analysis.report.line</field>
        <field name="arch" type="xml">
            <tree name="lims_analysis_report_line_tree" editable="bottom">
                <field name="sequence" widget="handle"/>
                <field name="analysis_id"/>
                <field name="analysis_state"/>
                <field name="analysis_stage_id"/>
            </tree>
        </field>
    </record>
</odoo>
