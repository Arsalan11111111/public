<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="create_analysis_request_wizard_form" model="ir.ui.view">
        <field name="name">Create analysis request wizard</field>
        <field name="model">create.analysis.request.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group col="12">
                        <field name="text" nolabel="1" readonly="1" colspan="12"/>
                    </group>
                </sheet>
                <footer>
                    <button string="Confirm" type="object" name="confirm" class="oe_highlight"/>
                    <button special="cancel" string="Cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>
