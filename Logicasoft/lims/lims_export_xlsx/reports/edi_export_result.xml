<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="report_edi_export_result_sop" model="ir.actions.report">
        <field name="binding_model_id" ref="lims_base.model_lims_sop"/>
        <field name="name">Edi export results</field>
        <field name="model">lims.sop</field>
        <field name="attachment_use" eval="False"/>
        <field name="report_type">openpyxl</field>
        <field name="report_name">lims_report_xls.report_edi_export_result_sop</field>
        <field name="report_file">lims_report_xls.report_edi_export_result</field>
        <field name="print_report_name">'Result_export_%s' % (object.display_name)</field>
    </record>
    <record id="report_edi_export_result_analysis" model="ir.actions.report">
        <field name="binding_model_id" ref="lims_base.model_lims_analysis"/>
        <field name="name">Edi export results</field>
        <field name="model">lims.analysis</field>
        <field name="attachment_use" eval="False"/>
        <field name="report_type">openpyxl</field>
        <field name="report_name">lims_report_xls.report_edi_export_result_analysis</field>
        <field name="report_file">lims_report_xls.report_edi_export_result</field>
        <field name="print_report_name">'Result_export_%s' % (object.display_name)</field>
    </record>
    <record id="report_edi_export_result_request" model="ir.actions.report">
        <field name="binding_model_id" ref="lims_base.model_lims_analysis_request"/>
        <field name="name">Edi export results</field>
        <field name="model">lims.analysis.request</field>
        <field name="attachment_use" eval="False"/>
        <field name="report_type">openpyxl</field>
        <field name="report_name">lims_report_xls.report_edi_export_result_request</field>
        <field name="report_file">lims_report_xls.report_edi_export_result</field>
        <field name="print_report_name">'Result_export_%s' % (object.display_name)</field>
    </record>

</odoo>