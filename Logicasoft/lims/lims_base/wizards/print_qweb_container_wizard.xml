<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="print_qweb_container_wizard_form" model="ir.ui.view">
        <field name="name">Print Qweb container</field>
        <field name="model">print.qweb.container.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <footer>
                        <button string="Print Container" type="object" name="print_container" class="oe_highlight"/>
                        <button special="cancel" string="Cancel"/>
                    </footer>
                </sheet>
            </form>
        </field>
    </record>

    <record id="print_qweb_container_wizard_action" model="ir.actions.act_window">
        <field name="name">Print Qweb container</field>
        <field name="res_model">print.qweb.container.wizard</field>
        <field name="view_mode">form</field>
        <field name="type">ir.actions.act_window</field>
    </record>

</odoo>