<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="lims_analysis_compute_result_form" model="ir.ui.view">
        <field name="name">Computed result</field>
        <field name="model">lims.analysis.compute.result</field>
        <field name="inherit_id" ref="lims_base.lims_analysis_compute_result_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='limit_compute_result_ids']/tree/field[@name='message']" position="after">
                <field name="pack_ids" widget="many2many_tags"/>
            </xpath>
        </field>
    </record>

</odoo>