<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="lims_analysis_limit_numeric_result_form" model="ir.ui.view">
        <field name="name">Analysis Limit Numeric Result</field>
        <field name="model">lims.analysis.limit.numeric.result</field>
        <field name="inherit_id" ref="lims_base.lims_analysis_limit_numeric_result_form"/>
        <field name="arch" type="xml">
            <field name="message" position="after">
                <field name="pack_ids" widget="many2many_tags"/>
            </field>
        </field>
    </record>

    <record id="lims_analysis_limit_numeric_result_tree" model="ir.ui.view">
        <field name="name">Analysis Limit Numeric Result</field>
        <field name="model">lims.analysis.limit.numeric.result</field>
        <field name="inherit_id" ref="lims_base.lims_analysis_limit_numeric_result_tree_view"/>
        <field name="arch" type="xml">
            <field name="message" position="after">
                <field name="pack_ids" widget="many2many_tags"/>
            </field>
        </field>
    </record>

    <record id="lims_analysis_limit_compute_result_form" model="ir.ui.view">
        <field name="name">Analysis Limit Compute Result</field>
        <field name="model">lims.analysis.limit.compute.result</field>
        <field name="inherit_id" ref="lims_base.lims_analysis_limit_compute_result_form"/>
        <field name="arch" type="xml">
            <field name="message" position="after">
                <field name="pack_ids" widget="many2many_tags"/>
            </field>
        </field>
    </record>

    <record id="lims_analysis_limit_compute_result_tree" model="ir.ui.view">
        <field name="name">Analysis Limit Compute Result</field>
        <field name="model">lims.analysis.limit.compute.result</field>
        <field name="inherit_id" ref="lims_base.lims_analysis_limit_compute_result_tree_view"/>
        <field name="arch" type="xml">
            <field name="message" position="after">
                <field name="pack_ids" widget="many2many_tags"/>
            </field>
        </field>
    </record>


</odoo>